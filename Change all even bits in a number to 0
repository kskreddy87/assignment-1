#include <bits/stdc++.h>
using namespace std;
class Solution {
  public:
    long long int convertEvenBitToZero(long long int n) 
    {
        return (n & 0xAAAAAAAA);
    }
};
int main() {
    int t;
    cin >> t;
    while (t--) {
        long long int n;
        cin >> n;
        Solution ob;
        cout << ob.convertEvenBitToZero(n) << endl;
    }
    return 0;
}
